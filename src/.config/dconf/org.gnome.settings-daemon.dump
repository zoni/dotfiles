[plugins/color]
night-light-enabled=true
night-light-temperature=uint32 3700

[plugins/media-keys]
custom-keybindings=['/org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom0/', '/org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom1/', '/org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom2/', '/org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom3/', '/org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom4/', '/org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom5/', '/org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom6/', '/org/gnome/settings-daemon/plugins/media-keys/custom-keybindings/custom7/']
email=['<Alt><Super>e']
home=['<Alt><Super>f']
rotate-video-lock-static=@as []
screenreader=@as []
screensaver=['<Super>Backspace']
search=['<Alt><Super>s']
terminal=@as []
www=['<Alt><Super>b']

[plugins/media-keys/custom-keybindings/custom0]
binding='<Super>semicolon'
command='ulauncher-toggle'
name='Ulauncher'

[plugins/media-keys/custom-keybindings/custom1]
binding='<Super>apostrophe'
command='wezterm-gui'
name='Wezterm'

[plugins/media-keys/custom-keybindings/custom2]
binding='<Super>backslash'
command='guake-toggle'
name='Toggle Guake'

[plugins/media-keys/custom-keybindings/custom3]
binding='<Alt><Super>t'
command='wezterm start zsh -i -c tsp'
name='Load tmux profile'

[plugins/media-keys/custom-keybindings/custom4]
binding='<Alt><Super>p'
command='1password --quick-access'
name='1Password'

[plugins/media-keys/custom-keybindings/custom5]
binding='<Alt><Super>o'
command='obsidian-fzf-or-launch'
name='Obsidian'

[plugins/media-keys/custom-keybindings/custom6]
binding='<Alt><Super>z'
command='zeal'
name='Zeal'

[plugins/media-keys/custom-keybindings/custom7]
binding='<Shift><Alt><Super>o'
command='wezterm start zsh -i -c "knowledgebase sitemap-select"'
name='Obsidian sitemap-select'

[plugins/power]
power-button-action='suspend'
sleep-inactive-ac-timeout=3600
sleep-inactive-ac-type='nothing'
sleep-inactive-battery-timeout=1800
sleep-inactive-battery-type='suspend'
